algoritmo "Variaveis"
// Função :
// Autor :
// Data : 11/18/2022
// Seção de Declarações 
var
    nome, sobreNome, nomeCompleto: literal
inicio
      Escreval("Informe o seu nome: ")
      leia(nome)
      Escreval("Informe o sobrenome: ")
      leia(sobreNome)
      nomeCompleto <- nome + " " + sobreNome
      Escreval(nomeCompleto)
fimalgoritmo
