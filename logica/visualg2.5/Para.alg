algoritmo "Para"
// Função :
// Autor :
// Data : 11/20/2022
// Seção de Declarações 
var
   i:inteiro
inicio
      i <- 0
      
      enquanto i <= 10 faca
               escreval(i)
               i <- i + 1
      fimenquanto
      
      repita
            escreval(i)
            i <- i + 1
      ate 1 > 10
      
fimalgoritmo
